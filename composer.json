{
	"name": "tomzx/composer-proxy",
	"description": "Composer proxy to cache external packages sources",
	"type": "library",
	"keywords": [
		"composer",
		"proxy"
	],
	"homepage": "https://github.com/tomzx/composer-proxy",
	"license": "MIT",
	"authors": [
		{
			"name": "Tom Rochette",
			"email": "tom@tomrochette.com",
			"homepage": "http://www.tomrochette.com"
		}
	],
	"support": {
		"issues": "https://github.com/tomzx/composer-proxy/issues"
	},
	"require": {
		"php": ">=5.4.0",
		"klein/klein": "~2.1",
		"composer/satis": "dev-master",
		"composer/composer": "1.0.*@dev"
	},
	"require-dev": {
		"phpunit/phpunit": "~4"
	},
	"autoload": {
		"psr-4": {
			"ComposerProxy\\": "src/ComposerProxy"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.1-dev"
		}
	}
}
